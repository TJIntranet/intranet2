<IfModule mod_rewrite.c>
	RewriteEngine On

	RewriteRule ^$ /var/www/iodine/core.php5 [L]

	RewriteRule ^www/ - [L]

	RewriteRule ^private - [L]

	RewriteRule ^webkdc-service - [L]

	RewriteRule ^webauth-status - [L]

	RewriteRule ^login - [L]

	RewriteRule ^(.*)$ /var/www/iodine/core.php5?$1?%{QUERY_STRING} [L]
</IfModule>
