<div class="intrabox" id="intrabox_[<$name>]" style="filter:alpha(opacity=90);" onMouseOver="this.style.filter='alpha(opacity=100)';" onMouseOut="this.style.filter='alpha(opacity=90)';">
<div class="boxbuttons">
 <a href="[<$I2_ROOT>]manageibox/move/[<$boxid>]/-1"><img src="[<$I2_ROOT>]www/pics/up.gif" alt="up" /></a>
 <a href="[<$I2_ROOT>]manageibox/move/[<$boxid>]/1"><img src="[<$I2_ROOT>]www/pics/down.gif" alt="down" /></a>
 <a href="[<$I2_ROOT>]manageibox/minimize/[<$boxid>]"><img src="[<$I2_ROOT>]www/pics/min.gif" alt="min" /></a>
 <a href="[<$I2_ROOT>]manageibox/delete/[<$boxid>]"><img src="[<$I2_ROOT>]www/pics/close.gif" alt="close" /></a>
</div>
<div class="boxheader" onDblClick="var content = document.getElementById('boxcontent_[<$name>]'); (content.style.display == 'none' ? content.style.display = 'block' : content.style.display = 'none');">[<$title>]</div>
<div class="boxcontent" id="boxcontent_[<$name>]">
