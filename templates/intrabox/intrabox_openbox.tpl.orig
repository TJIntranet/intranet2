<div class="intrabox" id="intrabox_[<$name>]" onMouseOver="this.style.filter='alpha(opacity=100)';" onMouseOut="this.style.filter='alpha(opacity=50)';">
<div class="boxheader" onDblClick="var content = document.getElementById('boxcontent_[<$name>]'); (content.style.display == 'none' ? content.style.display = 'block' : content.style.display = 'none');">[<$title>]</div>
<div class="boxcontent" id="boxcontent_[<$name>]">
