objectclass ( i2OC:1
	NAME ('tjhsstStudent' 'iodineStudent')
	SUP inetOrgPerson STRUCTURAL
	MUST (
		graduationYear $
		iodineUid $
		cn $
		sn $
		tjhsstStudentId $
		startpage $
		style $
		header $
		chrome
	)
	MAY (
		perm-showpictures $
		perm-showaddress $
		perm-showtelephone $
		perm-showbirthday $
		perm-showschedule $
		perm-showmap $
		perm-showpictures-self $
		perm-showaddress-self $
		perm-showtelephone-self $
		perm-showbirthday-self $
		perm-showschedule-self $
		perm-showmap-self $
		displayName $
		middlename $
		nickname $
		counselor $
		group $
		iodineUidNumber $
		birthday $
		sex $
		aim $
		yahoo $
		msn $
		jabber $
		icq $
		googleTalk $
		webpage $
		locker $
		startpage $
		soundexlast $
		soundexfirst $
		comments $
		freshmanPicture $
		sophomorePicture $
		juniorPicture $
		seniorPicture
	)
	)

objectclass ( i2OC:2
	NAME ('tjhsstTeacher' 'iodineTeacher' 'tjhsstStaff' 'tjhsstFaculty')
	SUP inetOrgPerson STRUCTURAL
	MUST (
		cn $
		iodineUid $
		sn
	)
	MAY (
		employeeNumber $
		displayName $
		middlename $
		group $
		nickname $
		iodineUidNumber $
		birthday $
		sex $
		startpage $
		header $
		style $
		chrome $
		startpage $
		soundexlast $
		soundexfirst
	)
	)

objectclass ( i2OC:3
	NAME ('tjhsstSection' 'section')
	MUST (
		tjhsstSectionId $
		classPeriod $
		associatedClassId
	)
	MAY (
		displayname $
		description $
		year $
		roomNumber $
		enrolledStudent $
		associatedTeacher
	)
	)

objectclass ( i2OC:4
	NAME ('tjhsstClass' 'tjhsstCourse')
